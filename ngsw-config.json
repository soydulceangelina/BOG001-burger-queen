{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/polyfills.*.js",
          "/runtime.*.js",
          "/*.js",
          "!/*-sw.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani|jpeg)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api",
      "urls": [
        "https://burger-queen-api.vercel.app/**",
        "https://firebasestorage.googleapis.com/v0/b/burgerqueen-ce419.appspot.com/o/**",
        "https://firestore.googleapis.com/**",
        "https://www.googleapis.com/**",
        "https://apis.google.com/js/**",
        "https://www.google.com/images"
      ],
      "cacheConfig": {
        "maxSize": 3,
        "maxAge": "1d",
        "strategy": "freshness",
        "timeout": "2s"
      }
    }
  ]
}
